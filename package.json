{
  "name": "soflow",
  "version": "0.0.1",
  "description": "Run local and swf workflows",
  "main": "lib/index.js",
  "author": "Trym Skaar <trym.skaar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-root-import": "^4.1.8",
    "blue-tape": "^1.0.0",
    "eslint": "^3.17.1",
    "eslint-config-skalar": "^0.0.7",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "archiver": "^1.3.0",
    "aws-sdk": "^2.23.0",
    "axios": "^0.15.3",
    "debug": "^2.6.1",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-runtime",
      "transform-class-properties",
      "transform-export-extensions",
      "babel-root-import"
    ],
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "skalar"
    ],
    "rules": {
      "react/prop-types": 0,
      "no-magic-numbers": 0,
      "no-process-env": 0,
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "no-invalid-this": 0
    }
  }
}
